{
  "name": "casflo-api-ts",
  "version": "1.0.0",
  "description": "High-performance TypeScript API for Casflo on Cloudflare Workers",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "dependencies": {
    "hono": "^4.4.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240512.0",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "wrangler": "^3.60.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}